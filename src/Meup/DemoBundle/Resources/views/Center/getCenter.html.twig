{% extends "MeupDemoBundle::layout.html.twig" %}

{% block title "Symfony - Center" %}

{% block content_header '' %}

{% block content %}
    <h1 class="title">Center #{{ id }}</h1>

    <p>{{ center }}</p>
    <p>
        <a href="{{ path('get_centers') }}">{{ 'center.list.link'|trans({}, 'MeupDemoBundle') }}</a> <br>
        [<a href="{{ path('remove_centers', {'id': id}) }}">{{ 'center.remove.link'|trans({}, 'MeupDemoBundle') }}</a>]
        [<a href="{{ path('edit_centers', {'id': id}) }}">{{ 'center.edit.link'|trans({}, 'MeupDemoBundle') }}</a>]
    </p>
{% endblock %}

{% set code = code(_self) %}
