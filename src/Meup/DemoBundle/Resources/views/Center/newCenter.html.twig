{% extends "MeupDemoBundle::layout.html.twig" %}

{% block title "Symfony - Centers" %}

{% block content_header '' %}

{% block content %}
    <h1 class="title">{{ 'center.new.headline'|trans({}, 'MeupDemoBundle') }}</h1>

    {%  if (form is defined) %}
    <form action="{{ url('post_centers') }}" method="POST" {{ form_enctype(form) }}>

        {{ form_widget(form) }}

        <input type="submit" value="{% trans from 'MeupDemoBundle' %}center.new.submit{% endtrans %}">
    </form>
    {% endif %}

{% endblock %}

{% set code = code(_self) %}
